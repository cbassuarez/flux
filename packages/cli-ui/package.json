{
  "name": "@flux-lang/cli-ui",
  "version": "0.1.19",
  "description": "Ink-based UI for the Flux CLI",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "npm run build --workspace @flux-lang/brand",
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "pretest": "npm run build --workspace @flux-lang/brand",
    "test": "vitest"
  },
  "dependencies": {
    "@flux-lang/brand": "workspace:^",
    "@flux-lang/cli-core": "workspace:^",
    "chalk": "^5.4.1",
    "ink": "^4.4.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "ink-testing-library": "^2.1.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
