{
  "name": "@flux-lang/viewer",
  "version": "0.1.11",
  "description": "Local web viewer for Flux documents.",
  "license": "MIT",
  "author": "Sebastian Suarez-Solis",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "editor-dist",
    "static",
    "README.md",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "prepare-editor": "npm run build --workspace @flux-lang/editor-ui --if-present && node ../../scripts/sync-editor-dist.mjs",
    "prebuild": "npm run build --workspace @flux-lang/brand && npm run prepare-editor",
    "build": "tsc -p tsconfig.json",
    "pretest": "npm run build --workspace @flux-lang/brand && npm run prepare-editor",
    "test": "vitest"
  },
  "dependencies": {
    "@flux-lang/brand": "workspace:^",
    "@flux-lang/core": "workspace:^",
    "@flux-lang/render-html": "workspace:^",
    "@flux-lang/typesetter": "workspace:^"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
